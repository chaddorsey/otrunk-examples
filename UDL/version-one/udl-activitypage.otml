          				<td border="1"><object refid="$item.icon.globalId" 
          						viewid="${icon_view}"/></td>
          				</tr>
          				
          				#end
          				</table>
          			</template>
          			<objectsToInsert>
						<OTUDLBackpackItem>
							<otobject>
								<OTCompoundDoc local_id="select_item_3">
									<bodyText>A compound doc</bodyText>
								</OTCompoundDoc>
							</otobject>
							<icon local_id="dot_icon">
								<OTImage local_id="dot" name="Dot"
								imageBytes="http://upload.wikimedia.org/wikipedia/commons/thumb/e/e0/Orange_pog.svg/64px-Orange_pog.svg.png" />
							</icon>
						</OTUDLBackpackItem>
						<OTUDLBackpackItem>
							<otobject>
								<OTCompoundDoc local_id="select_item_4">
									<bodyText>A compound doc</bodyText>
								</OTCompoundDoc>
							</otobject>
							<icon>
								<OTImage name="Flower"
								imageBytes="http://upload.wikimedia.org/wikipedia/commons/1/1f/RedFlower.png" />
							</icon>
						</OTUDLBackpackItem>
					</objectsToInsert>
