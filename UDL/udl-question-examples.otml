          </OTCompoundDoc>
          	<OTCompoundDoc name="OT example 1">
          		<bodyText>
          			This example uses OTUDLQuestions with no scripting required. The internals of these questions are OTCompoundDocs.<p/>
          			Here is the question:<br/>
          			<object refid="${udlquestions1}"/>
          		</bodyText>
          		<documentRefs>
          			<OTUDLQuestions local_id="udlquestions1">
														<questions>
															<OTCompoundDoc name="L5">
																<bodyText><div class="body">
																	<object refid="${text2}" />
																</div></bodyText>
																<documentRefs>
																	<OTText local_id="text2"/>
																</documentRefs>
															</OTCompoundDoc>
															<OTCompoundDoc name="L4">
																<bodyText><div class="body">
																(Hint: Think about surfaces that are slippery.)<p/>
																	<object refid="${text3}" />
																</div></bodyText>
																<documentRefs>
																	<OTText local_id="text3" />
																</documentRefs>
															</OTCompoundDoc>
															<OTCompoundDoc name="L3">
																<bodyText><div class="body">
																	<object refid="${text4}" />
																</div></bodyText>
																<documentRefs>
																	<OTText local_id="text4" text="I would sit on a slippery piece of ..."/>
																</documentRefs>
															</OTCompoundDoc>
															<OTCompoundDoc name="L2">
																<bodyText><div class="body">
																	<object refid="${radio1}" />
																</div></bodyText>
																<documentRefs>
																	<OTChoice local_id="radio1">
																		<choices>
																		<OTText name="I would sit on a piece of notebook paper" />
																		<OTText name="I would sit on a piece of wax paper" />
																		</choices>
																	</OTChoice>
																</documentRefs>
															</OTCompoundDoc>
															<OTCompoundDoc name="L1">
																<bodyText><div class="body">
																	<object refid="${text5}" />
																</div></bodyText>
																<documentRefs>
																	<OTText local_id="text5" text="I would sit on anything that is slippery, like wax paper, ice, etc."/>
																</documentRefs>
															</OTCompoundDoc>
														</questions>
													</OTUDLQuestions>
          		</documentRefs>
          	</OTCompoundDoc>
