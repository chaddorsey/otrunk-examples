												<OTUDLCompoundDoc local_id="pretest-1" name="1">
													<bodyText>
														<div class="buffer">
															<div class="border">
																<div class="body">
																	Answer the following questions. When you have finished, press the <b>submit</b>
																	button. Warning: after you hit the <b>submit</b> button, you will not be able
																	to go back and change your answers.<p/>
																	<ol>
																		<li>A hockey puck slides along an icy surface. What makes it slow down?<br />
																			<object refId="${question1-56}" /><p/></li>
																		<li>The rougher the surface, ...<br />
																			<object refId="${question2-56}" /><p/></li>
																		<li>Which of the following is an example of trying to increase friction?<br />
																			<object refId="${question3-56}" /><p/></li>
																		<li>Which of the following statements is <b>not</b> true?<br />
																			<object refId="${question4-56}" /><p/></li>
																	</ol>
																	<p/>
																	<table>
																		<tr>
																			<td>
																				Press the <b>Submit</b> button when you are done.
																			</td>
																			<td>
																				<object refid="${pretest-submit-button}"/>
																			</td>
																		</tr>
																	</table>
																	<object refid="${pretest-button-script}"/>
																</div>
															</div>
														</div>
													</bodyText>
													<documentRefs>
														<OTButton local_id="pretest-submit-button" name="Button" text="Submit"/>
														<OTScriptObject local_id="pretest-button-script">
											              <referenceObjects>
											                <object refid="${section-card-container}" />
											                <object refid="${pretest-submit-button}" />
											                <object refid="${section_intro}" />
											              </referenceObjects>
											              <script>
											                <OTJavascript src="set_button.js" />
											              </script>
											            </OTScriptObject>
														<OTChoice local_id="question1-56">
															<choices>
																<OTText name="It runs out of push" />
																<OTText name="It rubs against the ice" />
																<OTText name="In runs out of electricity" />
															</choices>
														</OTChoice>
														<OTChoice local_id="question2-56">
															<choices>
																<OTText name="the greater the friction" />
																<OTText name="the less the friction" />
																<OTText name="the same level of friction" />
															</choices>
														</OTChoice>
														<OTChoice local_id="question3-56">
															<choices>
																<OTText name="Squirting oil into door hinges" />
																<OTText name="Spreading grit on icy roads" />
																<OTText name="Waxing the underneath of skis" />
															</choices>
														</OTChoice>
														<OTChoice local_id="question4-56">
															<choices>
																<OTText name="Friction can only slow things down" />
																<OTText name="Friction is never useful" />
																<OTText name="Friction can stop things from moving" />
															</choices>
														</OTChoice>
													</documentRefs>
												</OTUDLCompoundDoc>
