													<choices>
													<OTText name="Finger" />
													<OTText name="Rubbing" />
													</choices>
												</OTChoice>
												<OTText local_id="text63"/>
												<OTChoice local_id="radio10">
													<choices>
													<OTText name="Rub it faster" />
													<OTText name="Add wax to make it more slippery" />
													<OTText name="Push down harder on the penny" />
