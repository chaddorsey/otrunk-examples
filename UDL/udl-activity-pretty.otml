								<OTDefinition>
									<word>friction</word>
									<definition>
										<OTCompoundDoc>
											<bodyText>
												<div class="buffer">
													<div class="definition-border">
														<div class="body">
															<b>Friction</b> is a force that slows the motion of objects that are touching.
														</div>
													</div>
												</div>
											</bodyText>
										</OTCompoundDoc>
									</definition>
								</OTDefinition>
								<OTDefinition>
									<word>sliding</word>
									<definition>
										<OTCompoundDoc>
											<bodyText>
												<div class="buffer">
													<div class="definition-border">
														<div class="body">
															<b>Sliding</b> is moving smoothly over a surface.
														</div>
													</div>
												</div>
											</bodyText>
										</OTCompoundDoc>
									</definition>
								</OTDefinition>
								<OTDefinition>
									<word>force</word>
									<definition>
										<OTCompoundDoc>
											<bodyText>
												<div class="buffer">
													<div class="definition-border">
														<div class="body">
															A <b>force</b> is a push or pull that tends to make an object move, stop, or change direction.
														</div>
													</div>
												</div>
											</bodyText>
										</OTCompoundDoc>
									</definition>
								</OTDefinition>
								<OTDefinition>
									<word>surface</word>
									<definition>
										<OTCompoundDoc>
											<bodyText>
												<div class="buffer">
													<div class="definition-border">
														<div class="body">
															A <b>surface</b> is the outside of an object or body.
														</div>
													</div>
												</div>
											</bodyText>
										</OTCompoundDoc>
									</definition>
								</OTDefinition>
								<OTDefinition>
									<word>computer model</word>
									<definition>
										<OTCompoundDoc>
											<bodyText><div class="body">
												A <b>computer model</b> is a program that runs on a computer and imitates the real world in some way.
											</div></bodyText>
										</OTCompoundDoc>
									</definition>
								</OTDefinition>
								<OTDefinition>
									<word>fricción</word>
									<definition>
										<OTCompoundDoc>
											<bodyText><div class="body">
												<b>Fricción:</b> La fuerza que hace decelerar a un objeto cuando se pasa sobre algúna superficie.
											</div></bodyText>
										</OTCompoundDoc>
									</definition>
								</OTDefinition>
								<OTDefinition>
									<word>superficie</word>
									<definition>
										<OTCompoundDoc>
											<bodyText><div class="body">
												<b>Superficie:</b> La superficie es la parte externa de un objeto. 
											</div></bodyText>
										</OTCompoundDoc>
									</definition>
								</OTDefinition>
								<OTDefinition>
